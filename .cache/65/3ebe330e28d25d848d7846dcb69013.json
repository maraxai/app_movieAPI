rt state-less class component\r\nexport class MovieCard extends React.Component {\r\n  render() {\r\n\r\n    // destructuring - assign 'movie' and 'onClick' as this components object\r\n    const { movie, onClick } = this.props;\r\n    // returns the movie title\r\n    return (\r\n      <Card bg=\"light\" style={{ width: '50rem' }}>\r\n        <Card.Img style={{ width: '200px'}} variant=\"top\" src={movie.imagePath} />\r\n        <Card.Body>\r\n          <Card.Title>{movie.title}</Card.Title>\r\n          <Card.Text>{movie.description}</Card.Text>\r\n          <Button onClick={() => onClick(movie)} variant=\"outline-secondary\" size=\"sm\">Open</Button>\r\n        </Card.Body>\r\n      </Card>\r\n    );\r\n  }\r\n}\r\n\r\nMovieCard.propTypes={\r\n  movie: PropTypes.shape({\r\n    title: PropTypes.string\r\n  }).isRequired,\r\n  onClick: PropTypes.func.isRequired\r\n};\r\n"},"lineCount":null}},"error":null,"hash":"473f14b7634501dc8cba0b282f12845c","cacheData":{"env":{}}}